<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
		@Styles.Render("~/css")
		@Styles.Render("~/Chess/css")
</head>
<body>
	<div id="header" class="navbar navbar-inverse navbar-fixed-top" style="position: relative;">
		<div class="container">
			<div class="navbar-header">
				@Html.ActionLink("Chess", "Index", "Home", null, new { @class = "navbar-brand" })
			</div>
			<div class="greeting-collapse-class navbar-collapse collapse">
				<ul class="nav navbar-nav">

				</ul>
				@Html.Partial("_LoginPartial")
			</div>
		</div>
	</div>
	@RenderBody()
	@RenderSection("scripts", required: false)
</body>
</html>
